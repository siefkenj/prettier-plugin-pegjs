{
    "name": "prettier-plugin-pegjs",
    "description": "A Prettier plugin for formatting Pegjs and Peggy-js grammars",
    "version": "2.0.1",
    "type": "module",
    "author": {
        "name": "Jason Siefken",
        "email": "siefkenj@gmail.com"
    },
    "keywords": [
        "pegjs",
        "parser",
        "prettier"
    ],
    "main": "build/prettier-plugin-pegjs.js",
    "files": [
        "build/**/*.js",
        "build/**/*.ts",
        "build/**/*.js.map"
    ],
    "scripts": {
        "test": "vitest",
        "watch": "mkdirp build; vite build --watch",
        "build": "rm -rf build; mkdirp build; tsc --build && vite build"
    },
    "dependencies": {
        "@types/node": "^20.5.6",
        "prettier": "^3.0.2"
    },
    "devDependencies": {
        "@types/prettier": "^3.0.0",
        "eslint": "^8.48.0",
        "jsdom": "^22.1.0",
        "mkdirp": "3.0.1",
        "peggy": "^3.0.2",
        "resolve-cwd": "^3.0.0",
        "vite": "^4.4.9",
        "vitest": "^0.34.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/siefkenj/prettier-plugin-pegjs.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/siefkenj/prettier-plugin-pegjs/issues"
    },
    "homepage": "https://github.com/siefkenj/prettier-plugin-pegjs#readme",
    "engines": {
        "node": ">= 16.0"
    }
}
